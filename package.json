  "dependencies": {
    "dom7": "^4.0.6",
    "framework7": "^8.0.5",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^8.0.5",
    "material-icons": "^1.13.7",
    "skeleton-elements": "^4.0.1",
    "svelte": "^3.59.1",
    "swiper": "^9.3.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^8.4.1",
    "sass": "^1.62.1",
    "vite": "^4.3.9"
  }
}
