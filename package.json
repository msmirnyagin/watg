{
  "name": "tg-webapp-templates",
  "private": true,
  "version": "1.0.0",
  "description": "tg webapp templates",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
